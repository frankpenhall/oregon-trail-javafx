<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TownspeopleController">
   <children>
      <Label alignment="CENTER" layoutY="75.0" prefHeight="23.0" prefWidth="750.0" text="Townspeople" />
      <Label alignment="CENTER" layoutX="6.0" layoutY="352.0" prefHeight="23.0" prefWidth="743.0" text="Do ya have enough room on that wagon of yours for another settler?" />
      <Label alignment="CENTER" layoutX="3.0" layoutY="381.0" prefHeight="23.0" prefWidth="743.0" text="I'll like to join you on the trail!" />
      <Button layoutX="286.0" layoutY="475.0" mnemonicParsing="false" onAction="#addSettler" text="Yes" />
      <Button layoutX="414.0" layoutY="475.0" mnemonicParsing="false" onAction="#noSettler" text="No" />
      <ImageView fitHeight="174.0" fitWidth="137.0" layoutX="316.0" layoutY="137.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/townsperson.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
