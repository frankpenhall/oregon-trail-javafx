<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.VisitSettlementController">
   <children>
      <Label layoutX="348.0" layoutY="103.0" text="Settlement" />
      <Label layoutX="281.0" layoutY="160.0" text="You have encountered a settlement!" />
      <Label layoutX="93.0" layoutY="209.0" text="While in town, you can buy supplies at the General Store, talk to the townspeople, or continue on your way " />
      <Button layoutX="186.0" layoutY="411.0" mnemonicParsing="false" onAction="#switchToStore2" text="General Store" />
      <Button layoutX="473.0" layoutY="411.0" mnemonicParsing="false" onAction="#switchToTripMenu" text="Continue on Trail" />
      <Button layoutX="331.0" layoutY="411.0" mnemonicParsing="false" onAction="#switchToPeople2" text="Talk to people" />
   </children>
</AnchorPane>
