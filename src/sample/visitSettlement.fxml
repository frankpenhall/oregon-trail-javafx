<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.VisitSettlementController">
   <children>
      <Label layoutX="338.0" layoutY="93.0" text="Settlement" />
      <Label layoutX="271.0" layoutY="150.0" text="You have encountered a settlement!" />
      <Label layoutX="83.0" layoutY="199.0" text="While in town, you can buy supplies at the General Store, talk to the townspeople, or continue on your way " />
      <Button layoutX="176.0" layoutY="401.0" mnemonicParsing="false" onAction="#switchToStore" text="General Store" />
      <Button layoutX="321.0" layoutY="401.0" mnemonicParsing="false" onAction="#switchToPeople" text="Talk to people" />
      <Button layoutX="463.0" layoutY="401.0" mnemonicParsing="false" onAction="#switchToTripMenu" text="Continue on Trail" />
   </children>
</AnchorPane>
