<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TownspeopleController">
   <children>
      <Label layoutX="339.0" layoutY="115.0" text="Townspeople" />
      <Label layoutX="188.0" layoutY="169.0" text="Do you have enough room on that wagon of yours for another settler?" />
      <Label layoutX="295.0" layoutY="198.0" text="I'll like to join you on the trail!" />
      <Button layoutX="358.0" layoutY="274.0" mnemonicParsing="false" onAction="#addSettler" text="Yes" />
      <Button layoutX="359.0" layoutY="338.0" mnemonicParsing="false" onAction="#noSettler" text="No" />
   </children>
</AnchorPane>
