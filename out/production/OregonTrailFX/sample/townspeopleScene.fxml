<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TownspeopleController">
   <children>
      <Label layoutX="338.0" layoutY="103.0" text="Townspeople" />
      <Label layoutX="185.0" layoutY="347.0" text="Do you have enough room on that wagon of yours for another settler?" />
      <Label layoutX="293.0" layoutY="376.0" text="I'll like to join you on the trail!" />
      <Button layoutX="321.0" layoutY="434.0" mnemonicParsing="false" onAction="#addSettler" text="Yes" />
      <Button layoutX="393.0" layoutY="434.0" mnemonicParsing="false" onAction="#noSettler" text="No" />
      <ImageView fitHeight="174.0" fitWidth="137.0" layoutX="313.0" layoutY="144.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/townsperson.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
