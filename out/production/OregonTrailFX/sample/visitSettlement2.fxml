<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.VisitSettlementController">
   <children>
      <Label layoutX="348.0" layoutY="103.0" text="Settlement" />
      <Label layoutX="279.0" layoutY="369.0" text="You have encountered a settlement!" />
      <Label layoutX="91.0" layoutY="397.0" text="While in town, you can buy supplies at the General Store, talk to the townspeople, or continue on your way " />
      <Button layoutX="333.0" layoutY="442.0" mnemonicParsing="false" onAction="#switchToStore2" text="General Store" />
      <Button layoutX="165.0" layoutY="442.0" mnemonicParsing="false" onAction="#switchToTripMenu" text="Continue on Trail" />
      <Button layoutX="486.0" layoutY="442.0" mnemonicParsing="false" onAction="#switchToPeople2" text="Talk to people" />
      <ImageView fitHeight="170.0" fitWidth="248.0" layoutX="251.0" layoutY="161.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/town.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
